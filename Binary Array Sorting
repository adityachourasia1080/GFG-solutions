void binSort(int A[], int N)
    {
       //Your code here
       
       /**************
        * No need to print the array
        * ************/
        int i=-1,j=0;
        while(j<N)
        {
            if (A[j]==0)
            {
                i++;
                swap(A[i],A[j]);
            }
            j++;
        }
    }
